package main;

public class MinBinHeap implements HeapInterface {

	  // in here go all your data and methods for the heap

	int size, current, end;
	EntryPair root;
	EntryPair[] heap;
	
	  public MinBinHeap ( ) { // default constructor
	    // explicitly include this
	    // we need to have the default constructor
	    // if you then write others, this one will still be there
		  size = 0;
		  end = 1;
		  heap = new EntryPair[100];
	  }
	  
	public MinBinHeap(EntryPair e){
		root = e;
		size++;
		end++;
		heap = new EntryPair[100];
	}

	@Override
	public void insert(EntryPair entry) {
		if(size == 0){
			heap[end] = entry;
			size++;
			end++;
		}
		else if(entry.getPriority() > heap[])
		
	}

	@Override
	public void delMin() {
		
		
	}

	@Override
	public EntryPair getMin() {
		return root;
	}

	@Override
	public int size() {
		return size;
	}

	@Override
	public void build(EntryPair[] entries) {
		// TODO Auto-generated method stub
		
	}
	}